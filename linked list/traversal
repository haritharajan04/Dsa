Class Node{ 
int data;
Node next;
Node(int data1){
 this.data = data1;
 this.next = null;
}
}:
public class Linkedlist{
private static Node convertArr2LL(int[] arr){
Node head = new Node(arr[i]);
Node mover = head;
for (int i = 1; i< arr.length; i++){
Node head = new Node(arr[i]);
mover.next = temp;
mover = temp;
}
return head;
}
public static boid main(Strings[] args) {
int[] arr = (12, 5, 6, 8);
Node head = convertArr2LL(arr);
Node temp = head;
while (temp != null){
System.out.print(temp.data + "");
temp = temp.next;
}
}
}
