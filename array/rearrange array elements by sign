import java.util.*;
public class Solution{
  public int[] rearrangeArray(int[] nums){
    int[] result = new int[n];
    for(int num: nums){
      if(num >= 0 && posIndex<n){
        result[posIndex]= num;
        posIndex += 2;
      } else if (num<0 && negIndex < n){
        result[negIndex] = num;
        negIndex += 2;
      }
    }
    return result;
  }
}
